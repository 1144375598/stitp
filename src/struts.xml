<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<constant name="struts.i18n.encoding" value="UTF-8"></constant>
	<package name="user" namespace="/user" extends="json-default">
	    <action name="outOfRange" class="com.njupt.stitp.server.action.UserAction"
			method="outOfRange">
			<result type="json"></result>
		</action>
		<action name="getUser" class="com.njupt.stitp.server.action.UserAction"
			method="getInfo">
			<result type="json"></result>
		</action>
		<action name="bumpRemind" class="com.njupt.stitp.server.action.UserAction"
			method="bumpRemind">
			<result type="json"></result>
		</action>
		<action name="lockScreen" class="com.njupt.stitp.server.action.UserAction"
			method="lockScreen">
			<result type="json"></result>
		</action>
		<action name="unlockScreen" class="com.njupt.stitp.server.action.UserAction"
			method="unlockScreen">
			<result type="json"></result>
		</action>
		<action name="continueUseTime" class="com.njupt.stitp.server.action.UserAction"
			method="continueUseTime">
			<result type="json"></result>
		</action>

		<action name="login" class="com.njupt.stitp.server.action.UserAction"
			method="login">
			<result type="json"></result>
		</action>

		<action name="register" class="com.njupt.stitp.server.action.UserAction"
			method="register">
			<result type="json"></result>
		</action>
		<action name="deleteChild" class="com.njupt.stitp.server.action.UserAction"
			method="deleteChild">
			<result type="json"></result>
		</action>
		<action name="uidAndCid" class="com.njupt.stitp.server.action.UserAction"
			method="uidAndCid">
			<result type="json"></result>
		</action>
		<action name="addFriend" class="com.njupt.stitp.server.action.UserAction"
			method="addFriend">
			<result type="json"></result>
		</action>
		<action name="addFriendResult" class="com.njupt.stitp.server.action.UserAction"
			method="addFriendResult">
			<result type="json"></result>
		</action>
		<action name="resetPassword" class="com.njupt.stitp.server.action.UserAction"
			method="resetPassword">
			<result type="json"></result>
		</action>
		<action name="getChild" class="com.njupt.stitp.server.action.UserAction"
			method="getChild">
			<result type="json"></result>
		</action>
		<action name="getValidation" class="com.njupt.stitp.server.action.UserAction"
		    method="getValidation">
		    <result type="json"></result>
		</action>
	</package>

	<package name="uploadInfo" namespace="/uploadInfo" extends="json-default">
		<action name="appInfo" class="com.njupt.stitp.server.action.UploadInfoAction"
			method="uploadAPPInfo">
			<result type="json"></result>
		</action>
		<action name="trackInfo" class="com.njupt.stitp.server.action.UploadInfoAction"
			method="uploadTrackInfo">
			<result type="json"></result>
		</action>
		<action name="useTimeControlInfo" class="com.njupt.stitp.server.action.UploadInfoAction"
			method="uploadUseTimeControlInfo">
			<result type="json"></result>
		</action>
		<action name="deleteUseTimeControl" class="com.njupt.stitp.server.action.UploadInfoAction"
			method="deleteUseTimeControl">
			<result type="json"></result>
		</action>
		<action name="geoFencingInfo" class="com.njupt.stitp.server.action.UploadInfoAction"
			method="uploadGeoFencingInfo">
			<result type="json"></result>
		</action>
	</package>

	<package name="downloadInfo" namespace="/downloadInfo" extends="json-default">
		<action name="appInfo" class="com.njupt.stitp.server.action.DownloadInfoAction"
			method="downloadAPPInfo">
			<result type="json"></result>
		</action>
		<action name="continueUseInfo" class="com.njupt.stitp.server.action.DownloadInfoAction"
			method="downloadContinueUseInfo">
			<result type="json"></result>
		</action>
		<action name="trackInfo" class="com.njupt.stitp.server.action.DownloadInfoAction"
			method="downloadTrackInfo">
			<result type="json"></result>
		</action>

		<action name="useTimeControlInfo" class="com.njupt.stitp.server.action.DownloadInfoAction"
			method="downloadUseTimeControlInfo">
			<result type="json"></result>
		</action>
		<action name="geoFencingInfo" class="com.njupt.stitp.server.action.DownloadInfoAction"
			method="downloadGeoFencingInfo">
			<result type="json"></result>
		</action>
	</package>
</struts>    
